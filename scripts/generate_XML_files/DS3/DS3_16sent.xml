            <pair e1="DS3.d3.s3.e2" e2="DS3.d3.s3.e3" id="DS3.d3.s3.i2" interaction="False" />
            <pair e1="DS3.d3.s3.e1" e2="DS3.d3.s3.e0" id="DS3.d3.s3.i3" interaction="False" />
