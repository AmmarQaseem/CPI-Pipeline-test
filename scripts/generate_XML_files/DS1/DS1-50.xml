    <document id="DS1.d2" origId="23122842">
        <sentence id="DS1.d2.s0" origId="23122842-0" text="Oxidative stress and cholinesterase inhibition in plasma, erythrocyte and brain of rats' pups following lactational exposure to malathion.">
            <entity id="DS1.d2.s0.e0" origId="Q9JKC1" charOffset="21-35" type="protein" text="cholinesterase"/>
            <entity id="DS1.d2.s0.e1" origId="22011568,4004" charOffset="128-137" type="compound" text="malathion"/>
            <pair e1="DS1.d2.s0.e1" e2="DS1.d2.s0.e0" id="DS1.d2.s0.i0" interaction="False" />
        </sentence>
        <sentence id="DS1.d2.s1" origId="23122842-6" text="We evaluated the activity of acetylcholinesterase (AChE) and butyrylcholinesterase (BChE), as well as on biochemical parameters related to the oxidative stress such lipoperoxidation and antioxidant enzyme activities as superoxide dismutase (SOD) and catalase (CAT) in the brain, plasma and erythrocytes of rats' pups at 21st postnatal day (Pnd).">
            <entity id="DS1.d2.s1.e0" origId="Q9JKC1" charOffset="84-88" type="protein" text="BChE"/>
            <entity id="DS1.d2.s1.e1" origId="5359597" charOffset="219-229" type="compound" text="superoxide"/>
            <pair e1="DS1.d2.s1.e1" e2="DS1.d2.s1.e0" id="DS1.d2.s1.i0" interaction="False" />
        </sentence>
        <sentence id="DS1.d2.s2" origId="23122842-8" text="Our results showed that the malathion exposure during lactation induced a high inhibitory effect of the brain, plasma and erythrocyte AChE and BChE activities in rat pups.">
            <entity id="DS1.d2.s2.e0" origId="22011568,4004" charOffset="28-37" type="compound" text="malathion"/>
            <entity id="DS1.d2.s2.e1" origId="Q9JKC1" charOffset="143-147" type="protein" text="BChE"/>
            <pair e1="DS1.d2.s2.e0" e2="DS1.d2.s2.e1" id="DS1.d2.s2.i0" interaction="False" />
        </sentence>
        <sentence id="DS1.d2.s3" origId="23122842-10" text="The present study shows, for the first time, that the exposure of postnatal pups to malathion via lactation inhibits the activity of brain, plasma and erythrocytes cholinesterase in the pups.">
            <entity id="DS1.d2.s3.e0" origId="22011568,4004" charOffset="84-93" type="compound" text="malathion"/>
            <entity id="DS1.d2.s3.e1" origId="Q9JKC1" charOffset="164-178" type="protein" text="cholinesterase"/>
            <pair e1="DS1.d2.s3.e0" e2="DS1.d2.s3.e1" id="DS1.d2.s3.i0" interaction="False" />
        </sentence>
    </document>
    <document id="DS1.d3" origId="23081707">
        <sentence id="DS1.d3.s0" origId="23081707-1" text="Environmental polychlorinated biphenyls (PCBs) are frequently bound onto nanoparticles (NPs).">
            <entity id="DS1.d3.s0.e0" origId="6636" charOffset="41-45" type="compound" text="PCBs"/>
            <entity id="DS1.d3.s0.e1" origId="P0C0P8" charOffset="88-91" type="protein" text="NPs"/>
            <pair e1="DS1.d3.s0.e0" e2="DS1.d3.s0.e1" id="DS1.d3.s0.i0" interaction="False" />
        </sentence>
        <sentence id="DS1.d3.s1" origId="23081707-3" text="The aim of this study was to study the hypothesis that binding PCBs to silica NPs potentiates PCB-induced cerebrovascular toxicity and brain damage in an experimental stroke model.">
            <entity id="DS1.d3.s1.e0" origId="6636" charOffset="63-67" type="compound" text="PCBs"/>
            <entity id="DS1.d3.s1.e1" origId="24261" charOffset="71-77" type="compound" text="silica"/>
            <entity id="DS1.d3.s1.e2" origId="P0C0P8" charOffset="78-81" type="protein" text="NPs"/>
            <pair e1="DS1.d3.s1.e1" e2="DS1.d3.s1.e2" id="DS1.d3.s1.i0" interaction="False" />
            <pair e1="DS1.d3.s1.e0" e2="DS1.d3.s1.e2" id="DS1.d3.s1.i1" interaction="False" />
        </sentence>
        <sentence id="DS1.d3.s2" origId="23081707-4" text="Mice (C57BL/6, males, 12-week-old) were exposed to PCB153 bound to NPs (PCB153-NPs), PCB153, or vehicle.">
            <entity id="DS1.d3.s2.e0" origId="37034" charOffset="51-57" type="compound" text="PCB153"/>
            <entity id="DS1.d3.s2.e1" origId="P0C0P8" charOffset="67-70" type="protein" text="NPs"/>
            <entity id="DS1.d3.s2.e2" origId="P0C0P8" charOffset="72-82" type="protein" text="PCB153-NPs"/>
            <entity id="DS1.d3.s2.e3" origId="37034" charOffset="72-78" type="compound" text="PCB153"/>
            <pair e1="DS1.d3.s2.e0" e2="DS1.d3.s2.e2" id="DS1.d3.s2.i0" interaction="False" />
            <pair e1="DS1.d3.s2.e0" e2="DS1.d3.s2.e1" id="DS1.d3.s2.i1" interaction="False" />
        </sentence>
        <sentence id="DS1.d3.s3" origId="23081707-10" text="A long-term (30 days) oral exposure to PCB153-NPs resulted in a higher PCB153 content in the abdominal adipose tissue and amplified adhesion of leukocytes to the brain endothelium as compared to treatment with PCB153 alone.">
            <entity id="DS1.d3.s3.e0" origId="P0C0P8" charOffset="39-49" type="protein" text="PCB153-NPs"/>
            <entity id="DS1.d3.s3.e1" origId="37034" charOffset="39-45" type="compound" text="PCB153"/>
            <entity id="DS1.d3.s3.e2" origId="37034" charOffset="71-77" type="compound" text="PCB153"/>
            <pair e1="DS1.d3.s3.e1" e2="DS1.d3.s3.e0" id="DS1.d3.s3.i0" interaction="False" />
        </sentence>
        <sentence id="DS1.d3.s4" origId="23081707-11" text="This study provides the first evidence that binding to NPs increases cerebrovascular toxicity of environmental toxicants, such as PCB153.">
            <entity id="DS1.d3.s4.e0" origId="P0C0P8" charOffset="55-58" type="protein" text="NPs"/>
            <entity id="DS1.d3.s4.e1" origId="37034" charOffset="130-136" type="compound" text="PCB153"/>
            <pair e1="DS1.d3.s4.e1" e2="DS1.d3.s4.e0" id="DS1.d3.s4.i0" interaction="False" />
        </sentence>
    </document>
    <document id="DS1.d4" origId="23806243">
        <sentence id="DS1.d4.s0" origId="23806243-4" text="The vaccines included: pneumococcal (PPSV), influenza (Flu), tetanus diphtheria (Td), tetanus diphtheria pertussis (Tdap), Human Papilloma Virus (HPV), hepatitis A (HepA), hepatitis B (HepB) and herpes zoster (shingles).">
            <entity id="DS1.d4.s0.e0" origId="O75800" charOffset="55-58" type="protein" text="Flu"/>
            <entity id="DS1.d4.s0.e1" origId="4177956" charOffset="185-189" type="compound" text="HepB"/>
            <pair e1="DS1.d4.s0.e1" e2="DS1.d4.s0.e0" id="DS1.d4.s0.i0" interaction="False" />
        </sentence>
        <sentence id="DS1.d4.s1" origId="23806243-10" text="For PPSV and Flu those 65+ had low specificity compared to patients of younger ages while those in the youngest age group had lowest specificity for HepA and HepB.">
            <entity id="DS1.d4.s1.e0" origId="O75800" charOffset="13-16" type="protein" text="Flu"/>
