package org.learningformat.transform.dependency.transformer;

import org.learningformat.transform.dependency.DependencyGraph;

/**
 * This is the simplest kind of transformer, as nothing happens
 * @author philippe
 *
 */
public class IdentityTransformer  implements DependencyGraphTransformer{

	
	
	@Override
	public DependencyGraph transform(DependencyGraph dg) {		
		dg.setName("identity");
		return dg;
	}

}
