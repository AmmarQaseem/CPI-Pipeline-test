package org.learningformat.impl;

import org.learningformat.api.Entity;
import org.learningformat.api.Pair;

public class DefaultPair extends DefaultElement implements Pair {
	protected Entity e1;
	protected Entity e2;
	protected boolean interaction;
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Interaction#getE1()
	 */
	public Entity getE1() {
		return e1;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Interaction#setE1(java.lang.String)
	 */
	public void setE1(Entity e1) {
		this.e1 = e1;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Interaction#getE2()
	 */
	public Entity getE2() {
		return e2;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Interaction#setE2(java.lang.String)
	 */
	public void setE2(Entity e2) {
		this.e2 = e2;
	}
	public boolean isInteraction() {
		return interaction;
	}
	public void setInteraction(boolean interaction) {
		this.interaction = interaction;
	}
}
