package org.learningformat.impl;

import org.learningformat.api.Element;

public abstract class DefaultElement implements Element {
	
	protected String id;
	protected String origId;
	protected String type;

	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Element#getId()
	 */
	public String getId() {
		return id;
	}

	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Element#setId(java.lang.String)
	 */
	public void setId(String id) {
		this.id = id;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Entity#getOrigId()
	 */
	public String getOrigId() {
		return origId;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Entity#setOrigId(java.lang.String)
	 */
	public void setOrigId(String origId) {
		this.origId = origId;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Entity#getType()
	 */
	public String getType() {
		return type;
	}
	/* (non-Javadoc)
	 * @see org.unifiedformat.impl.Entity#setType(java.lang.String)
	 */
	public void setType(String type) {
		this.type = type;
	}
	
	@Override
	public String toString() {
		return super.toString() + "\n" + 
		"  id = '" + id + "'\n"
		;
	}

}
